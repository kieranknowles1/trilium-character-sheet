{
  "name": "trilium-character-sheet",
  "version": "1.0.0",
  "description": "This is a repository based on my [NixOS Configuration](https://github.com/kieranknowles1/nixcfg).",
  "scripts": {
    "schemas": "typescript-json-schema --required --noExtraProps --defaultNumberType integer tsconfig.json Character > dist/CharacterSchema.json",
    "build": "tsc && webpack",
    "pack": "npm run schemas && npm run build && tpack"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/trilium": "npm:trilium-types@^0.1.2",
    "trilium-pack": "^0.1.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "typescript-json-schema": "^0.65.1",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4"
  }
}
